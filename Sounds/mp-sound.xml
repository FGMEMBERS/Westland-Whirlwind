<?xml version="1.0" encoding="ISO-8859-1"?>
 
<PropertyList>

  <fx>
 
    <!-- Engine Left -->
    <crank> 
      <name>engstart</name>
      <path>Aircraft/Westland-Whirlwind/Sounds/starter.wav</path>
      <property>/engines/engine[0]/cranking</property>
      <volume> 
        <property>/orientation/alpha-deg</property>
        <type>log</type>
        <min>0.5</min>
        <max>1.0</max>
      </volume>
      <volume> 
        <factor>1.2</factor>
        <max>1.0</max>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </crank>

    <crank> 
      <name>cough</name>
      <path>Sounds/coughing.wav</path>
      <property>/engines/engine[0]/running</property>
      <volume> 
        <property>/orientation/alpha-deg</property>
        <type>log</type>
        <min>0.5</min>
        <max>1.0</max>
      </volume>
      <volume> 
        <factor>1.2</factor>
        <max>1.0</max>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </crank>

    <engine> 
      <name>engine idle rpm</name>
      <mode>looped</mode>
      <path>Aircraft/Westland-Whirlwind/Sounds/running1.wav</path>
      <condition> 
        <property>/engines/engine[0]/running</property>
       <less-than> 
          <property>/engines/engine[0]/rpm</property>
          <value>1100</value>
        </less-than>
        <greater-than> 
          <property>/engines/engine[0]/rpm</property>
          <value>150</value>
        </greater-than>
      </condition>
      <volume> 
        <property>/orientation/alpha-deg</property>
        <type>log</type>
        <min>1.0</min>
        <max>1.5</max>
      </volume>
      <volume> 
        <property>/sim/current-view/internal</property>
        <offset>1.0</offset>
        <factor>-0.4</factor>
      </volume>
      <pitch> 
        <property>/engines/engine[0]/rpm</property>
        <factor>0.0009496</factor>
        <min>0.6</min>
        <max>2.0</max>
        <offset>0</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </engine>

    <engine-2> 
      <name>engine low rpm</name>
      <mode>looped</mode>
      <path>Aircraft/Westland-Whirlwind/Sounds/running3.wav</path>
      <condition> 
        <property>/engines/engine[0]/running</property>
        <greater-than-equals> 
          <property>/engines/engine[0]/rpm</property>
          <value>1100</value>
        </greater-than-equals>
        <less-than> 
          <property>/engines/engine[0]/rpm</property>
          <value>2200</value>
        </less-than>
      </condition>
      <volume> 
        <property>/orientation/alpha-deg</property>
        <type>log</type>
        <min>1.0</min>
        <max>1.5</max>
      </volume>
      <volume> 
        <property>/sim/current-view/internal</property>
        <offset>1.0</offset>
        <factor>-0.4</factor>
      </volume>
      <pitch> 
        <property>/engines/engine[0]/rpm</property>
        <factor>0.0006697</factor>
        <min>0.3</min>
        <max>2.0</max>
        <offset>0</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </engine-2>

    <engine-4> 
      <name>engine high rpm</name>
      <mode>looped</mode>
      <path>Aircraft/Westland-Whirlwind/Sounds/running2.wav</path>
      <condition> 
        <property>/engines/engine[0]/running</property>
        <greater-than-equals> 
          <property>/engines/engine[0]/rpm</property>
          <value>2200</value>
        </greater-than-equals>
      </condition>
      <volume> 
       <property>/orientation/alpha-deg</property>
        <type>log</type>
        <min>1.0</min>
        <max>1.5</max>
      </volume>
      <volume> 
        <property>/sim/current-view/internal</property>
        <offset>1.0</offset>
        <factor>-0.4</factor>
      </volume>
      <pitch> 
        <property>/engines/engine[0]/rpm</property>
        <factor>0.000462</factor>
        <min>0.3</min>
        <max>2.0</max>
        <offset>0</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </engine-4>

    <engine-0> 
      <name>engine shut down rpm</name>
      <path>Aircraft/Westland-Whirlwind/Sounds/shutdown.wav</path>
      <condition> 
        <property>/engines/engine[0]/running</property>
        <not> 
          <property>controls/engines/engine[0]/cutoff</property>
        </not>
        <less-than-equals> 
          <property>/engines/engine[0]/rpm</property>
          <value>150</value>
        </less-than-equals>
      </condition>
      <volume> 
        <property>/orientation/alpha-deg</property>
        <type>log</type>
        <min>1.0</min>
        <max>1.5</max>
      </volume>
      <volume> 
        <property>/sim/current-view/internal</property>
        <offset>1.0</offset>
        <factor>-0.4</factor>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </engine-0>

    <!-- Engine 2 -->
    <crank> 
      <name>engstart</name>
      <path>Aircraft/Westland-Whirlwind/Sounds/starter.wav</path>
      <property>/engines/engine[1]/cranking</property>
      <volume> 
        <property>/orientation/alpha-deg</property>
        <type>log</type>
        <min>0.5</min>
        <max>1.0</max>
      </volume>
      <volume> 
        <factor>1.2</factor>
        <max>1.0</max>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </crank>

    <crank> 
      <name>cough</name>
      <path>Sounds/coughing.wav</path>
      <property>/engines/engine[1]/running</property>
      <volume> 
        <property>/orientation/alpha-deg</property>
        <type>log</type>
        <min>0.5</min>
        <max>1.0</max>
      </volume>
      <volume> 
        <factor>1.2</factor>
        <max>1.0</max>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </crank>

    <engine> 
      <name>engine idle rpm</name>
      <mode>looped</mode>
      <path>Aircraft/Westland-Whirlwind/Sounds/running1.wav</path>
      <condition> 
        <property>/engines/engine[1]/running</property>
       <less-than> 
          <property>/engines/engine[1]/rpm</property>
          <value>1100</value>
        </less-than>
        <greater-than> 
          <property>/engines/engine[1]/rpm</property>
          <value>150</value>
        </greater-than>
      </condition>
      <volume> 
        <property>/orientation/alpha-deg</property>
        <type>log</type>
        <min>1.0</min>
        <max>1.5</max>
      </volume>
      <volume> 
        <property>/sim/current-view/internal</property>
        <offset>1.0</offset>
        <factor>-0.4</factor>
      </volume>
      <pitch> 
        <property>/engines/engine[1]/rpm</property>
        <factor>0.0009496</factor>
        <min>0.6</min>
        <max>2.0</max>
        <offset>0</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </engine>

    <engine-2> 
      <name>engine low rpm</name>
      <mode>looped</mode>
      <path>Aircraft/Westland-Whirlwind/Sounds/running3.wav</path>
      <condition> 
        <property>/engines/engine[1]/running</property>
        <greater-than-equals> 
          <property>/engines/engine[1]/rpm</property>
          <value>1100</value>
        </greater-than-equals>
        <less-than> 
          <property>/engines/engine[1]/rpm</property>
          <value>2200</value>
        </less-than>
      </condition>
      <volume> 
        <property>/orientation/alpha-deg</property>
        <type>log</type>
        <min>1.0</min>
        <max>1.5</max>
      </volume>
      <volume> 
        <property>/sim/current-view/internal</property>
        <offset>1.0</offset>
        <factor>-0.4</factor>
      </volume>
      <pitch> 
        <property>/engines/engine[1]/rpm</property>
        <factor>0.0006697</factor>
        <min>0.3</min>
        <max>2.0</max>
        <offset>0</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </engine-2>

    <engine-4> 
      <name>engine high rpm</name>
      <mode>looped</mode>
      <path>Aircraft/Westland-Whirlwind/Sounds/running2.wav</path>
      <condition> 
        <property>/engines/engine[1]/running</property>
        <greater-than-equals> 
          <property>/engines/engine[1]/rpm</property>
          <value>2200</value>
        </greater-than-equals>
      </condition>
      <volume> 
       <property>/orientation/alpha-deg</property>
        <type>log</type>
        <min>1.0</min>
        <max>1.5</max>
      </volume>
      <volume> 
        <property>/sim/current-view/internal</property>
        <offset>1.0</offset>
        <factor>-0.4</factor>
      </volume>
      <pitch> 
        <property>/engines/engine[1]/rpm</property>
        <factor>0.000462</factor>
        <min>0.3</min>
        <max>2.0</max>
        <offset>0</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </engine-4>

    <engine-0> 
      <name>engine shut down rpm</name>
      <path>Aircraft/Westland-Whirlwind/Sounds/shutdown.wav</path>
      <condition> 
        <property>/engines/engine[1]/running</property>
        <not> 
          <property>controls/engines/engine[1]/cutoff</property>
        </not>
        <less-than-equals> 
          <property>/engines/engine[1]/rpm</property>
          <value>150</value>
        </less-than-equals>
      </condition>
      <volume> 
        <property>/orientation/alpha-deg</property>
        <type>log</type>
        <min>1.0</min>
        <max>1.5</max>
      </volume>
      <volume> 
        <property>/sim/current-view/internal</property>
        <offset>1.0</offset>
        <factor>-0.4</factor>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </engine-0>

    <rumble>
      <name>rumble</name>
      <mode>looped</mode>
      <path>Sounds/rumble.wav</path>
      <condition>
        <or>
          <property>/gear/gear[0]/wow</property>
          <property>/gear/gear[1]/wow</property>
          <property>/gear/gear[2]/wow</property>
        </or>
      </condition>
      <volume>
        <property>/velocities/airspeed-kt</property>
        <type>log</type>
        <factor>0.5</factor>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </rumble>

    <gear>
      <name>gear</name>
      <mode>in-transit</mode>
      <path>Sounds/gear.wav</path>
      <property>/gear/gear[0]/position-norm</property>
      <pitch>
        <internal>dt_play</internal>
        <offset>1.0</offset>
        <factor>-0.001</factor>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </gear>
 
    <gear>
      <name>gear-lock</name>
      <path>Sounds/gear-lck.wav</path>
      <condition>
        <greather-than>
          <property>/gear/gear[0]/position-norm</property>
          <value>0.01</value>
        </greather-than>
        <less-than>
          <property>/gear/gear[0]/position-norm</property>
          <value>0.1</value>
        </less-than>
      </condition>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </gear>

    <squeal>
      <name>squeal</name>
      <path>Sounds/squeal.wav</path>
      <condition>
        <or>
          <property>/gear/gear[0]/wow</property>
          <property>/gear/gear[1]/wow</property>
          <property>/gear/gear[2]/wow</property>
        </or>
      </condition>
      <volume>
        <property>/velocities/speed-down-fps</property>
        <factor>0.05</factor>
        <min>0.1</min>
      </volume>
      <volume>
        <property>/velocities/airspeed-kt</property>
        <factor>0.01</factor>
        <offset>0.4</offset>
      </volume>
      <pitch>
        <offset>1.25</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </squeal>
 
    <flap>
      <name>flaps</name>
      <mode>in-transit</mode>
      <path>Sounds/flaps.wav</path>
      <property>/surface-positions/flap-pos-norm</property>
      <volume>
        <offset>0.25</offset>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </flap>

    <wind>
      <name>wind</name>
      <mode>looped</mode>
      <path>Sounds/wind.wav</path>
      <property>/velocities/airspeed-kt</property>
      <volume>
        <property>/position/altitude-ft</property>
        <factor>-0.000015</factor>
        <offset>1.0</offset>
        <min>0.1</min>
        <max>1.0</max>
      </volume>
      <volume>
        <property>/velocities/airspeed-kt</property>
        <factor>0.0015</factor>
        <min>0.03</min>
        <max>0.25</max>
      </volume>
      <pitch>
        <property>/velocities/airspeed-kt</property>
        <factor>0.01</factor>
        <offset>1.25</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </wind>
 
    <stall>
      <name>stall</name>
      <path>Sounds/stall.wav</path>
      <property>/sim/aero/alarms/stall-warning</property>
      <volume>
        <property>/velocities/airspeed-kt</property>
        <min>30.0</min>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </stall>
 
    <canopy> 
      <name>canopy slide</name>
      <mode>in-transit</mode>
      <path>Aircraft/Westland-Whirlwind/Sounds/canopy_slide.wav</path>
      <property>instrumentation/doors/crew/position-norm</property>
      <volume> 
        <factor>0.3</factor>
      </volume>
      <reference-dist>10.0</reference-dist>
      <max-dist>20.0</max-dist>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </canopy>

    <guns> 
      <name>gunfire</name>
      <mode>looped</mode>
      <path>Aircraft/Westland-Whirlwind/Sounds/gunfire.wav</path>
      <condition> 
        <property>controls/armament/trigger</property>
        <property>ai/submodels/submodel/count</property>
      </condition>
      <volume> 
        <factor>1.2</factor>
        <max>1.0</max>
      </volume>
    </guns>

  </fx>
 
</PropertyList>
